pipeline{
 
  agent any
  
  stages{
    stage("build"){
      steps{
        eccho 'building application...'
      }
    }
  }
  
   stages{
    stage("test"){
      steps{
        eccho 'testing application...'
      }
    }
  }
  
   stages{
    stage("deploy"){
      steps{
        eccho 'deploying application...'
      }
    }
  }
}

